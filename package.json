{
  "name": "grind-queue",
  "version": "0.8.0-beta.8",
  "description": "Queue provider for Grind",
  "author": "Shaun Harrison (@shnhrrsn)",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/grindjs/queue.git"
  },
  "keywords": [
    "grindjs",
    "grind-framework",
    "rabbitmq",
    "beanstalk",
    "ampq",
    "queue",
    "faktory"
  ],
  "peerDependencies": {
    "grind-cli": "^0.7.0",
    "grind-framework": "^0.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/register": "^7.0.0",
    "amqplib": "^0.5.1",
    "ava": "^1.0.0-rc.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-grind": "^0.8.0-beta.3",
    "eslint": "^5.8.0",
    "eslint-config-grind": "^2.1.5",
    "eslint-plugin-import-auto-name": "^1.1.0",
    "faktory-worker": "^0.6.3",
    "fivebeans": "^1.5.0",
    "grind-cli": "^0.7.0",
    "grind-framework": "^0.7.1"
  },
  "engines": {
    "node": ">=10.13"
  },
  "scripts": {
    "prepack": "bin/build",
    "lint": "eslint src",
    "test": "ava --verbose"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      [
        "grind",
        {
          "import-auto-name": {
            "autoresolve": true
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import-auto-name"
    ],
    "env": {
      "node": true
    },
    "extends": "grind",
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
